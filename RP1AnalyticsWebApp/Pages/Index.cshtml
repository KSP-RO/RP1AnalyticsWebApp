@page
@model rp1_analytics_server.IndexModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "RP-1 Analytics";
}

<div>
    <label for="Career">Select a career</label>
    <select asp-for="Career" asp-items="Model.Careers" onchange="getCareerLogs(this.value);" class="browser-default">
        <option value=""></option>
    </select>
</div>

<div id="chart"></div>

@section Scripts {
    <script src="~/js/careerLog.js" asp-append-version="true"></script>
}
